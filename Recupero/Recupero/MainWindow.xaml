<Window x:Class="Recupero.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Recupero"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="squadraIn" HorizontalAlignment="Left" Margin="84,112,0,0" TextWrapping="Wrap" Text="Squadra in casa" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="squadraOut" HorizontalAlignment="Left" Margin="84,155,0,0" TextWrapping="Wrap" Text="Squadra fuori casa" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="anno" HorizontalAlignment="Left" Margin="84,202,0,0" TextWrapping="Wrap" Text="anno" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="risultato" HorizontalAlignment="Left" Margin="84,248,0,0" TextWrapping="Wrap" Text="risultato" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="nominativi" HorizontalAlignment="Left" Margin="84,288,0,0" TextWrapping="Wrap" Text="nominativi marcatori" VerticalAlignment="Top" Width="120"/>
        <Button Content="crea partita" HorizontalAlignment="Left" Margin="84,333,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>

    </Grid>
</Window>
